{
    "builds": [{
        "src": "social/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static",
        "config": { "distDir": "static" }
    }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    }
  ]
}
